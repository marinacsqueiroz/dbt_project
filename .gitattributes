# Ignore só artefatos do dbt (não o projeto inteiro)
jaffle_shop/target/**                 linguist-vendored
jaffle_shop/logs/**                   linguist-generated
jaffle_shop/dbt_packages/**           linguist-vendored
jaffle_shop/dbt_internal_packages/**  linguist-vendored

# Conte SQL e YAML como linguagens detectáveis
jaffle_shop/**/*.sql   linguist-language=SQL   linguist-detectable=true
jaffle_shop/**/*.yml   linguist-language=YAML  linguist-detectable=true
jaffle_shop/**/*.yaml  linguist-language=YAML  linguist-detectable=true
